{
  "name": "sls-ts-api-template",
  "version": "0.0.0",
  "description": "Template for a serverless REST API with TypeScript and DynamoDB",
  "sideEffects": false,
  "main": "src/index.js",
  "scripts": {
    "build": "sls package",
    "deploy": "sls deploy",
    "deploy:prod": "sls deploy --stage prod",
    "dev:init": "sls dynamodb install",
    "dynamodb:start": "sls dynamodb start --stage local",
    "dynamodb:remove": "sls dynamodb remove --stage local",
    "format": "prettier --write './{,**/}*.[jt]s'",
    "start": "SLS_DEBUG=* AWS_REGION=us-west-2 sls offline start --stage local --httpPort 3000",
    "start:debug": "AWS_REGION=us-west-2 node --inspect=9220 ./node_modules/serverless/bin/serverless.js offline start --stage local",
    "logs:dev": "sls logs -f app -t --startTime 10m --stage dev"
  },
  "dependencies": {
    "@graphql-tools/merge": "^8.2.10",
    "@graphql-tools/schema": "^8.3.10",
    "core-js": "^3.23.5",
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "dynamodb": "^1.3.0",
    "dynamoose": "^2.8.6",
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "express-winston": "^4.2.0",
    "graphql": "^16.4.0",
    "graphql-middleware": "^6.1.25",
    "graphql-playground-middleware-express": "^1.7.23",
    "graphql-scalars": "^1.17.0",
    "graphql-shield": "^7.5.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "serverless": "^2.72.3",
    "serverless-http": "^2.7.0",
    "uuid": "^8.3.2",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "prettier": "^2.6.2",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-offline": "^7.1.0",
    "serverless-webpack": "^5.7.0",
    "ts-loader": "^9.2.9",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0"
  },
  "author": "",
  "license": "MIT"
}
